@code {
    // Specifies the orientation type of Menu component.
    // Specifies the orientation type of Menu component.
    // Specify the value of Sidebar component state. It indicates whether the sidebar component is in an open/close state.
    // public bool SidebarToggle = false;
    // [CascadingParameter]
    // public HttpContext? HttpContext { get; set; }

    [CascadingParameter]
    public HttpContext? HttpContext { get; set; }

    SfSidebar sidebarObj;
    [Inject] protected AccountApiClient accountApiClient { get; set; }
    [Inject] protected RedisUserRepository redisUserRepo { get; set; }
    [Inject] protected IEventBus eventBus { get; set; }
    public UserProfileViewModel? userProfileViewModel = null;
    private UpdatePasswordViewModel changePasswordViewModel = new UpdatePasswordViewModel();

    private string themeName;
    private bool Visibility { get; set; } = false;

    private SfUploader uploadObj { get; set; }
    public string CssClass = "custom-file";
    private string base64 { get; set; }
    private string fileSize { get; set; }
    private string ToastPosition = "Right";
    public bool IsFormValid = false;
    public bool IsFormValidChangePassword = false;

    private IBrowserFile selectedFile;
    private EditContext editContext;
    private EditContext changePasswordContext;
    private bool isSubmitting = false;
    //End:Hidden
    private async Task OnChange(UploadChangeEventArgs args)
    {
        base64 = string.Empty;
        foreach (var file in args.Files)
        {
            selectedFile = file.File;
            MemoryStream memoryStream = new MemoryStream();
            using var fileStream = file.File.OpenReadStream(long.MaxValue);
            await fileStream.CopyToAsync(memoryStream);
            byte[] bytes = memoryStream.ToArray();
            base64 = "data:image/png;base64," + Convert.ToBase64String(bytes);
            fileSize = await uploadObj.BytesToSizeAsync(file.FileInfo.Size);
        }
    }

    private async Task onFileRemove()
    {
        await uploadObj.RemoveAsync();
        base64 = string.Empty;
        fileSize = string.Empty;
    }

    HttpClient httpClient = new HttpClient();
    private void OnClicked()
    {
        this.Visibility = !this.Visibility;
    }
    SfDialog DialogObj;

    SfDialog ProfileDialogObj;

    private SfToast toast;


    private async Task HandleValidSubmit()
    {
        if (!changePasswordContext.Validate())
        {
            return;
        }

        if (isSubmitting)
        {
            return;
        }

        isSubmitting = true;

        try
        {
            // Your form submission logic here
            var response = await accountApiClient.UpdatePassword(changePasswordViewModel);
            if (response.IsSuccessStatusCode)
            {

                await ProfileDialogObj.HideAsync();

                var toastModel = new ToastModel
                {
                    Title = "Success",
                    Content = response.Message,
                    CssClass = "e-toast-success"
                };
                await toast.ShowAsync(toastModel);
            }
            else
            {
                var toastModel = new ToastModel
                {
                    Title = "Error",
                    Content = response.Message,
                    CssClass = "e-toast-danger"
                };
                await toast.ShowAsync(toastModel);
            }
        }
        finally
        {
            isSubmitting = false;
        }
    }

    private async Task HandleValidSubmitProfile()
    {

        if (!editContext.Validate())
        {
            return;
        }

        using var stream = selectedFile?.OpenReadStream();
        var response = await accountApiClient.UploadProfilePictureWithDataAsync(stream!, selectedFile?.Name, userProfileViewModel);
        if (response.IsSuccessStatusCode)
        {

            await ProfileDialogObj.HideAsync();

            await sessionStorage.RemoveItemAsync("userProfileData");

            var toastModel = new ToastModel
            {
                Title = "Success",
                Content = response.Message,
                CssClass = "e-toast-success"
            };
            await toast.ShowAsync(toastModel);
        }
        else
        {
            var toastModel = new ToastModel
            {
                Title = "Error",
                Content = response.Message,
                CssClass = "e-toast-danger"
            };
            await toast.ShowAsync(toastModel);
        }
    }

    private async void cancelclick(object args)
    {
        await DialogObj.HideAsync();
    }

    private async void cancelprofileclick(object args)
    {
        await ProfileDialogObj.HideAsync();
    }


    private async void select(MenuEventArgs args)
    {
        if (args.Item.Text == "Logout")
        {
            await DialogObj.ShowAsync();
        }

        if (args.Item.Text == "User")
        {
            changePasswordViewModel = new UpdatePasswordViewModel();
            await ProfileDialogObj.ShowAsync();
        }
    }

    private async void ToogleDarkMode()
    {
        userProfileViewModel = await sessionStorage.GetItemAsync<UserProfileViewModel>("userProfileData");

        userProfileViewModel!.IsDarkMode = !userProfileViewModel.IsDarkMode;

        appState.IsDarkMode = userProfileViewModel.IsDarkMode;
        await redisUserRepo.UpdateUserProfileAsync(userProfileViewModel);

        UpdateProfileEvent updateProfileEvent = new UpdateProfileEvent(userProfileViewModel);

        await eventBus.PublishAsync(updateProfileEvent);

        await sessionStorage.SetItemAsStringAsync("userProfileData", JsonConvert.SerializeObject(userProfileViewModel));
        _navigationManager.Refresh(true);

    }

    private void HandleFieldChanged(object sender, FieldChangedEventArgs e)
    {
        IsFormValid = editContext.Validate();
    }

    private void HandlePasswordFieldChanged(object sender, FieldChangedEventArgs e)
    {
        IsFormValidChangePassword = changePasswordContext.Validate();
    }


    protected override async void OnInitialized()
    {

        userProfileViewModel = await sessionStorage.GetItemAsync<UserProfileViewModel>("userProfileData");
        if (userProfileViewModel == null)
        {
            var userProfile = await accountApiClient.GetUserProfile();

            userProfileViewModel = userProfile.Result;

            appState.IsDarkMode = userProfileViewModel.IsDarkMode;

            appState.IsNavOpen = userProfileViewModel.IsNavOpen;


            await sessionStorage.SetItemAsStringAsync("userProfileData", JsonConvert.SerializeObject(userProfile.Result));

            editContext = new EditContext(userProfileViewModel);
            editContext.OnFieldChanged += HandleFieldChanged;
            IsFormValid = editContext.Validate();

            changePasswordContext = new EditContext(changePasswordViewModel);
            changePasswordContext.OnFieldChanged += HandlePasswordFieldChanged;
            // IsFormValidChangePassword = changePasswordContext.Validate();

            _navigationManager.NavigateTo(userProfile.Result.LastPageVisited);
        }

        _navigationManager.Refresh();
    }

    private string GetUri()
    {
        var uri = UrlHelper.ToAbsoluteUri(UrlHelper.Uri);
        return uri.AbsolutePath;
    }

    private void OverlayClick(OverlayModalClickEventArgs args)
    {
        this.Visibility = false;
    }

    public void Dispose()
    {
        throw new NotImplementedException();
    }

    public async void ToolbarClicked(Syncfusion.Blazor.Navigations.ClickEventArgs args)

    {
        userProfileViewModel!.IsNavOpen = !userProfileViewModel.IsNavOpen;

        appState.IsNavOpen = userProfileViewModel.IsNavOpen;

        var data1123456 = await redisUserRepo.UpdateUserProfileAsync(userProfileViewModel);

        UpdateProfileEvent updateProfileEvent = new UpdateProfileEvent(userProfileViewModel);

        await eventBus.PublishAsync(updateProfileEvent);
    }
    // public async void Toggle()
    // {

